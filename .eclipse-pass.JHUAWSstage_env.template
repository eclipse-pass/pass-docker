###################################################
# AWS resources will be accessed via IAM Roles ####
###################################################

AWS_REGION=us-east-1

###################################################
# PASS_CORE config ################################
###################################################
PASS_CORE_BASE_URL=https://stage.pass
PASS_CORE_BACKEND_USER=stagepasscoreuser
PASS_CORE_BACKEND_PASSWORD=stagepasscorepassword

PASS_CORE_POSTGRES_PORT=5432
PASS_CORE_DATABASE_URL=jdbc:postgresql://stage-postgres:5432/pass
PASS_CORE_DATABASE_USERNAME=stagedbuser
PASS_CORE_DATABASE_PASSWORD=stagedbpass

PASS_CORE_FILE_SERVICE_TYPE=S3
PASS_CORE_S3_BUCKET_NAME=pass-core-files
PASS_CORE_S3_REPO_PREFIX=stage

###################################################
# PASS_DEPOSIT_SERVICES config ####################
###################################################
